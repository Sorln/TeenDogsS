local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Teen Canines Script      (Sorln#3037)", "BloodTheme")
local Tab = Window:NewTab("Free GP Charthers")
local Section = Tab:NewSection("Lets you Spawn as Gamepass Charthers for free")
Section:NewButton("Deucalion", "ButtonInfo", function()
    print("Clicked")
    local args = {
    [1] = "Deucalion"
}

game:GetService("ReplicatedStorage").Appearance:InvokeServer(unpack(args))
end)
Section:NewButton("Kali", "ButtonInfo", function()
    print("Clicked")
    local args = {
    [1] = "Kali"
