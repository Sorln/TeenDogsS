local Section = Tab:NewSection("With Great Power Come's Well Power")
Section:NewButton("Regain all energy fast click when low", "ButtonInfo", function()
    print("Clicked")
    local args = {
    [1] = -99999
}

game:GetService("ReplicatedStorage").EnergyDrain:FireServer(unpack(args))
end)
Section:NewButton("Scream", "ButtonInfo", function()
    print("Clicked")
    game:GetService("ReplicatedStorage").Moves.Scream:FireServer()
